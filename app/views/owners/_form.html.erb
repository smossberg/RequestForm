<%= ownerable.name %>
<%= form_for [ownerable,Owner.new], remote: true do|f| %>
	<p>
		<%= f.label :firstname %>
		<%= f.text_field :firstname %>
		<%= f.label :lastname %>
		<%= f.text_field :lastname %>
	</p>
	<p>
		<%= f.label :email%>
		<%= f.email_field :email %>
	</p>
		<%= f.label :phone %>
		<%= f.telephone_field :phone %>
	</p>

	<p>
		<%= f.submit 'Add', class: 'btn btn-primary' %>
	</p>

<% end %>

